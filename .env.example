# Application Configuration
FREERANGE_APP_ENVIRONMENT=development
FREERANGE_APP_DEBUG=true
FREERANGE_APP_LOG_LEVEL=debug

# Server Configuration
FREERANGE_SERVER_PORT=8080

# Database Configuration (Elasticsearch)
# FREERANGE_DATABASE_URLS=http://localhost:9200
FREERANGE_DATABASE_USERNAME=
FREERANGE_DATABASE_PASSWORD=

# Redis Configuration
FREERANGE_REDIS_HOST=localhost
FREERANGE_REDIS_PORT=6379
FREERANGE_REDIS_PASSWORD=

# Provider API Keys (Keep these secret!)
FCM_SERVER_KEY=your-fcm-server-key
FCM_PROJECT_ID=your-fcm-project-id

APNS_KEY_ID=your-apns-key-id
APNS_TEAM_ID=your-apns-team-id
APNS_BUNDLE_ID=your-app-bundle-id
APNS_KEY_PATH=/path/to/apns/key.p8

SENDGRID_API_KEY=your-sendgrid-api-key
SENDGRID_FROM_EMAIL=noreply@yourdomain.com
SENDGRID_FROM_NAME=YourApp

TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_FROM_NUMBER=+1234567890

# Security Configuration
JWT_SECRET=your-super-secret-jwt-key-change-in-production

# Monitoring
FREERANGE_MONITORING_ENABLED=true